{
  "name": "KyanJukebox",
  "version": "0.0.1",
  "description": "Kyan Jukebox",
  "repository": "",
  "main": "js/app.js",
  "engines": {
    "node": "^6.2.0"
  },
  "dependencies": {
    "babel-plugin-transform-class-properties": "^6.8.0",
    "babel-preset-react": "^6.5.0",
    "babelify": "^7.3.0",
    "classnames": "^2.1.3",
    "flux": "^2.0.1",
    "keymirror": "~0.1.0",
    "object-assign": "^1.0.0",
    "react": "^15.0.2",
    "react-dom": "^15.0.2",
    "react-router": "^2.4.1"
  },
  "devDependencies": {
    "babel-cli": "^6.8.0",
    "babel-core": "^6.9.1",
    "babel-plugin-__coverage__": "^1.11.111",
    "babel-preset-es2015": "^6.9.0",
    "browserify": "^13.0.1",
    "envify": "^3.0.0",
    "immutable": "^3.8.1",
    "istanbul": "^0.4.3",
    "jasmine-core": "^2.4.1",
    "karma": "^0.13.22",
    "karma-babel-preprocessor": "^6.0.1",
    "karma-browserify": "^5.0.5",
    "karma-coverage": "^1.0.0",
    "karma-jasmine": "^1.0.2",
    "karma-phantomjs-launcher": "^1.0.0",
    "phantomjs-prebuilt": "^2.1.7",
    "reactify": "^0.15.2",
    "uglify-js": "~2.4.15",
    "watchify": "^3.7.0"
  },
  "scripts": {
    "start": "watchify -t babelify -v -d js/app.js -o js/bundle.js",
    "build": "browserify . -t [envify --NODE_ENV production] -t [babelify] | uglifyjs -cm > js/bundle.min.js",
    "test": "./node_modules/karma/bin/karma start --auto-watch --no-single-run karma.config.js",
    "test_once": "./node_modules/karma/bin/karma start --no-auto-watch karma.config.js --single-run"
  },
  "babel": {
    "plugins": [
      "transform-class-properties",
      [
        "__coverage__",
        {
          "only": "js/"
        }
      ]
    ],
    "presets": [
      "es2015",
      "react"
    ]
  },
  "author": "Kyan",
  "browserify": {
    "transform": [
      "reactify"
    ]
  }
}
